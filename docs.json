{
    "$schema": "https://mintlify.com/docs.json",
    "name": "Saros Docs",
    "logo": {
        "dark": "/logo/logo-dark.svg",
        "light": "/logo/logo-light.svg",
        "href": "https://saros.xyz/"
    },
    "favicon": "/favicon.svg",
    "theme": "linden",
    "colors": {
        "primary": "#6946E2",
        "light": "#6946E2",
        "dark": "#6946E2"
